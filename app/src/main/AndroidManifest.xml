            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="gov.dol.childlabor.CountryViewActivity" />
        </activity>
        <activity
            android:name=".ConventionActivity"
            android:label="@string/title_activity_convention"
